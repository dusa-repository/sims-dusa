											<listitem>
												<listcell label="${each.cedula}">
												</listcell>
												<listcell label="${each.primerNombre}">
												</listcell>
												<listcell label="${each.parentescoFamiliar}">
												</listcell>
											</listitem>
										</template>
									</listbox>
								</tabpanel>
							</tabpanels>
						</tabbox>
					</tabpanel>
					<tabpanel>
